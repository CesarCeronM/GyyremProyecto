<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gyyrem.gui.FXMLverMiembrosController">
   <children>
      <Label fx:id="lblTitulo" layoutY="-1.0" prefHeight="21.0" prefWidth="600.0" style="-fx-border-color: black; -fx-background-color: #32476E;" text="          VerMiembros" textFill="WHITE">
         <font>
            <Font name="SansSerif Regular" size="15.0" />
         </font>
      </Label>
      <Label fx:id="lblMiembros" layoutX="243.0" layoutY="43.0" text="Miembros">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TableView fx:id="tblMiembros" layoutX="35.0" layoutY="89.0" prefHeight="200.0" prefWidth="531.0">
        <columns>
          <TableColumn fx:id="colNombre" prefWidth="141.0" text="Nombre" />
          <TableColumn fx:id="colApellidoPaterno" prefWidth="121.0" text="Apellido Paterno" />
            <TableColumn fx:id="colApellidoMaterno" minWidth="0.0" prefWidth="125.0" text="Apellido Materno" />
            <TableColumn fx:id="colTelefono" prefWidth="143.0" text="Telefono" />
        </columns>
      </TableView>
      <Button fx:id="btnModificar" layoutX="399.0" layoutY="321.0" mnemonicParsing="false" onAction="#modificarMiembro" style="-fx-background-color: #32476E;" text="Modificar" textFill="WHITE" />
      <Button fx:id="btnRegresar" layoutX="140.0" layoutY="321.0" mnemonicParsing="false" onAction="#regresarVentanaMenu" style="-fx-background-color: #32476E;" text="Regresar" textFill="WHITE" />
      <Button fx:id="btnSuspender" layoutX="264.0" layoutY="321.0" mnemonicParsing="false" onAction="#suspenderMiembro" style="-fx-background-color: #32476E;" text="Suspender" textFill="WHITE" />
   </children>
</AnchorPane>
